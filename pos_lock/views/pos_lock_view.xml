<?xml version="1.0"?>
<odoo>

    <record model="ir.ui.view" id="res_group_view_pos_config_inherit">
        <field name="name">pos.config.inherit</field>
        <field name="model">pos.config</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="point_of_sale.pos_config_view_form" />
        <field name="arch" type="xml">
            <div id="posbox_reference" position="after">
                <h2>Group Configurtion</h2>
                <div class="row mt16 o_settings_container">
                    <div class="col-xs-12 col-md-6 o_setting_box">
                        <div class="o_setting_right_pane">
                            <div class="row">
                                <label  for="group_pos_user" class="col-md-3 o_light_label"/>
                                <field name="group_pos_user"/>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </field>
    </record>

</odoo>