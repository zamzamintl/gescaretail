<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="princess_product_composition_product_product_template_form_view" model="ir.ui.view">
        <field name="name">princess_product_composition.product.product.template.form.view</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='group_standard_price']" position="inside">
                <field name="composition_tag_ids" widget="many2many_tags"/>
            </xpath>
        </field>
    </record>
</odoo>
