<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets" inherit_id="point_of_sale.assets">
        <xpath expr="." position="inside">
            <script type="text/javascript"
                    src="/pos_product_search_view/static/src/js/pos_product_search_view.js"/>
        </xpath>
        <xpath expr="//link[@id='pos-stylesheet']" position="after">
            <link rel="stylesheet"
                  href="/pos_product_search_view/static/src/css/pos_product_search_view.css"/>
        </xpath>
    </template>
</odoo>