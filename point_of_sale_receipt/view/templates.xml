<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets" inherit_id="point_of_sale.assets">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/point_of_sale_receipt/static/lib/jquery-barcode.js"></script>
            <script type="text/javascript" src="/point_of_sale_receipt/static/src/js/point_of_sale_receipt.js"/>
        </xpath>
        <xpath expr="//link[@id='pos-stylesheet']" position="after">
            <link rel="stylesheet" href="/point_of_sale_receipt/static/src/css/point_of_sale_receipt.css"/>
        </xpath>
    </template>
</odoo>