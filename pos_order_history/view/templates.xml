<odoo>
    <data>
        <template id="pos_order_history_assets_backend" name="pos_order_history assets" inherit_id="point_of_sale.assets">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/pos_order_history/static/src/js/pos_order_history.js"></script>
            </xpath>
            <xpath expr="//link[@id='pos-stylesheet']" position="after">
                <link rel="stylesheet" href="/pos_order_history/static/src/css/pos.css" id="pos-order-history-pos-stylesheet" />
            </xpath>
        </template>
    </data>
</odoo>